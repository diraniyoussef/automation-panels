<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#cad4c7" />
            <gradient android:angle="-90" android:endColor="#dfe5dd" android:startColor="#b5c3b1" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#cad4c7" />
            <solid android:color="#95a990"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#d4dcd2" />
            <gradient android:angle="-90" android:endColor="#e9eee8" android:startColor="#b5c3b1" />
        </shape>
    </item>
</selector>